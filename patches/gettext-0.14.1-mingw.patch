+me=`echo "$0" | sed -e 's,.*[\\/],,'`
+
+help="\
+Try \`$me --help' for more information."
+
+exit_missing_arg="\
+echo \"$me: option \\\`\$1' requires an argument\" >&2
+echo \"\$help\" >&2
+exit 1"
+
+# Set up variables
+unset opt_client
+unset opt_server
+unset opt_hotspot
+unset opt_classpath
+unset opt_D
+unset opt_verbose
+unset opt_version
+unset opt_showversion
+unset opt_help
+unset opt_X
+unset opt_ea
+unset opt_da
+unset opt_esa
+unset opt_dsa
+unset opt_stop
+unset extraargs
+# Parse command line.  We have to do this in case the user
+# specifies the input file or uses stdin.  If so, then
+# we must accommodate...but we can only do that if we
+# parse ALL possible options.  This gets ugly if the
+# two versions of autoconf accept different options.
+while test $# -gt 0 ; do
+  optarg=`expr "x$1" : 'x-[^=]*=\(.*\)' \| \
+               "x$1" : 'x-.\(.*\)'`
+  case $1 in
+
+    -client )
+       opt_client="-client"
+       shift ;;
+    -server )
+       opt_server="-server"
+       shift ;;
+    -hotspot )
+       opt_hotspot="-hotspot"
+       shift ;;
+
+    -classpath=* | -cp=* )
+       opt_classpath="-classpath `cygpath -w -a -p $optarg`"
+       shift ;;
+    -classpath | -cp )
+       test $# = 1 && eval "$exit_missing_arg"
+       shift
+       opt_classpath="-classpath `cygpath -w -a -p $1`"
+       shift ;;
+
+    -D*=* ) 
+       prop=`echo $1 | sed -e 's/-D\([^=]*\)=[^=]*/\1/'`
+       value=`echo $1 | sed -e 's/-D[^=]*=\([^=]*\)/\1/'`
+       opt_D="$opt_D -D$prop=$value"
+       shift ;;
+
+    -verbose )
+       opt_verbose="-verbose"
+       shift ;;
+    -verbose:class )
+       opt_verbose="-verbose:class"
+       shift ;;
+    -verbose:gc )
+       opt_verbose="-verbose:gc"
+       shift ;;
+    -verbose:jni )
+       opt_verbose="-verbose:jni"
+       shift ;;
+
+    -version )
+       opt_version="-version"
+       shift ;;
+    -showversion )
+       opt_showversion="-showversion"
+       shift ;;
+
+    -help | -? )
+       opt_help="-help"
+       shift ;;
+    -X )
+       opt_X="-X"
+       shift ;;
+    -X* )
+       opt_X="-X${optarg}"
+       shift ;;
+
+    -ea | -enableassertions )
+       opt_ea="-ea"
+       shift ;;
+    -ea:* | -enableassertions:* )
+       opt_ea="-ea:${optarg}"
+       shift ;;
+		 
+    -da | -disableassertions )
+       opt_da="-da"
+       shift ;;
+    -da:* | -disableassertions:* )
+       opt_da="-da:${optarg}"
+       shift ;;
+
+    -esa | -enablesystemassertions )
+       opt_esa="-esa"
+       shift ;;
+    -dsa | -disablesystemassertions )
+       opt_dsa="-dsa"
+       shift ;;
+
+    -- )     # Stop option processing
+       shift
+       opt_stop="--"
+       break ;;
+
+    -* )
+       exec >&2
+       echo "$me: invalid option $1"
+       echo "$help"
+       exit 1 ;;
+
+    * ) 
+	    extraargs="$extraargs $1"
+		 shift
+       break ;;
+  esac
+done

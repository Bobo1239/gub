+# -*- shell-script -*-
+
+# copy and include extra mknetrel stuff
+. $src/$cygwin/mknetrel.extras || . $mknetrel/extras
+
+sover=3
+sub_packages="devel lib"
+depends="libintl3"
+lib_depends="libltdl3"
+build_depends="mingw-devel libtool-devel libiconv-devel"
+
+lib_name () {
+    echo libintl$sover
+}
+
+preconfig () {
+    extras_preconfig
+    cd $build || exit 1
+    sed -i~ -e 's/ac_cv_func_select=yes/ac_cv_func_select=no/' \
+	$build/config.cache
+    iscygwin || cat <<EOF >> config.cache
+# gettext does not include winsock2.h -- do not feel like patching right now
+# but in mingw only if winsock2.h
+ac_cv_func_select=\${ac_cv_func_select=no}
+# only in additional library -- do not feel like patching right now
+gl_cv_func_mbrtowc=\${gl_cv_func_mbrtowc=no}
+jm_cv_func_mbrtowc=\${jm_cv_func_mbrtowc=no}
+EOF
+}
+
+prebuild () {
+    extras_prebuild
+
+    # prevent reconfig trigger, that will fail with `environment changed'
+    # non-problem.
+    mkdir -p $build/config
+}
+
+devel_split () {
+    extras_devel_split
+    mkdir -p ./$prefix/share
+    mv $inst/$prefix/share/gettext ./$prefix/share
+}

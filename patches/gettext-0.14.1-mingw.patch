+#!/bin/sh -x
+cp INSTALL INSTALL.save
+cp COPYING COPYING.save
+aclocal -I config/m4
+libtoolize --automake --copy --force
+automake --gnu --add-missing --copy --force-missing
+autoconf
+mv INSTALL.save INSTALL
+mv COPYING.save COPYING
+
+cd autoconf-lib-link
+aclocal  -I m4 -I ../gettext-tools/m4 -I ../autoconf-lib-link/m4 -I ../config/m4
+automake --gnu
+autoheader
+autoconf
+
+cd ../gettext-runtime
+aclocal  -I m4 -I ../gettext-tools/m4 -I ../autoconf-lib-link/m4 -I ../config/m4
+automake --gnu
+autoheader
+autoconf
+
+cd ../gettext-runtime/libasprintf
+autoreconf  -I m4 -I ../gettext-tools/m4 -I ../autoconf-lib-link/m4 -I ../config/m4 
+
+cd ../../gettext-tools
+aclocal  -I m4 -I ../gettext-runtime/m4 -I ../autoconf-lib-link/m4 -I ../config/m4
+automake --gnu
+autoheader
+autoconf
+
